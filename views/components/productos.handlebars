<div class="container">
<div class="row">
{{#each imagenes }}
<div class="cartas col-3">
  <h5 class=" card-title">{{this.name}}</h5>
  <img class="card-img"src="{{this.url_image}}" class="card-img-top" alt="Card image cap">
  <div class="container">
  <div class="card-body row">
    <p class="card-text col-6">${{this.price}}</p>
    <button type="button" class="btn btn-outline-light col-6" onclick="añadir('{{this}}')"><img class="add" src="data:image/png;base64," alt=""></button>
  </div>
  </div>
 </div>
{{/each}}
</div>

</div>

<script>
    async function añadir(producto) {
        let nombre = producto;
        const response = await fetch("http://localhost:3003/producto", {
            method: "post",
            body: JSON.stringify({
                nombre,
            }),
        });
    };
</script>
<style>
    .cartas {
        margin-top: 20px;
    }
    .add {
      width: 20%;
    }
</style>